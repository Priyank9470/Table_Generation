
@{
    ViewData["Title"] = "GenerateTimetable";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>GenerateTimetable</h1>
@model string[,]

<table>
    <thead>
        <tr>
            @for (int i = 0; i < Model.GetLength(1); i++)
            {
                <th>Day @(i + 1)</th>
            }
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.GetLength(0); i++)
        {
            <tr>
                @for (int j = 0; j < Model.GetLength(1); j++)
                {
                    <td>@Model[i, j]</td>
                }
            </tr>
        }
    </tbody>
</table>

